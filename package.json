{
  "name": "global-store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 9000",
    "build": "tsc -b && vite build",
    "build:reactive-store": "npm run build:reactive-store:vanilla:dev && npm run build:reactive-store:vanilla:min",
    "build:reactive-store:dev": "vite build --config reactive-store/vite.config.reactive-store.ts",
    "build:reactive-store:min": "vite build --config reactive-store/vite.config.reactive-store.min.ts",
    "build:reactive-store:vanilla": "npm run build:reactive-store:vanilla:dev && npm run build:reactive-store:vanilla:min",
    "build:reactive-store:vanilla:dev": "vite build --config reactive-store/vite.config.reactive-store.vanilla.ts",
    "build:reactive-store:vanilla:min": "vite build --config reactive-store/vite.config.reactive-store.vanilla.min.ts",
    "build:reactive-store:iife": "npm run build:reactive-store:iife:dev && npm run build:reactive-store:iife:min",
    "build:reactive-store:iife:dev": "vite build --config reactive-store/vite.config.reactive-store.iife.ts",
    "build:reactive-store:iife:min": "vite build --config reactive-store/vite.config.reactive-store.iife.min.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "debounce-promise": "^3.1.2",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
